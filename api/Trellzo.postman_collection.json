		{
			"name": "Refresh access token",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"const promisify = (fn) => {",
							"    return new Promise((resolve, reject) => {",
							"        fn(resolve, reject);",
							"    });",
							"};",
							"",
							"const prevCookie = pm.variables.get('auth');",
							"const expectedCode = pm.variables.get('exp_code');",
							"",
							"pm.test(`Status code is ${expectedCode}`, function () {",
							"    pm.response.to.have.status(expectedCode);",
							"});",
							"",
							"if(expectedCode === 200)",
							"    pm.test('Access token changed', async () => {",
							"        const cookieJar = pm.cookies.jar();",
							"        await promisify((res, rej) => {",
							"            cookieJar.get('https://localhost', 'auth', ",
							"                (err, cook) => {",
							"                    if(err){",
							"                        console.error(`Could not retrieve auth cookie: ${err}`);",
							"                        return rej(err);",
							"                    }",
							"                    const coookieChanged = prevCookie != cook;",
							"                    pm.expect(coookieChanged).to.be.true;",
							"                    return res();",
							"            });",
							"        }).catch((err) => {",
							"            throw err;",
							"        });",
							"    })",
							"",
							"const requestsData = pm.variables.get('requestsData');",
							"pm.variables.clear()",
							"pm.variables.set('requestsData', requestsData);"
						],
						"type": "text/javascript"
					}
				},
				{
					"listen": "prerequest",
					"script": {
						"exec": [
							""
						],
