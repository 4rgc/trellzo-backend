					"listen": "prerequest",
					"script": {
						"exec": [
							"const name = pm.variables.get('name')",
							"const email = pm.variables.get('email')",
							"",
							"const req = JSON.parse(pm.request.body.raw)",
							"",
							"if (!name) {",
							"    const newReq = Object.assign({}, req);",
							"    delete newReq.name",
							"    pm.request.body.raw = JSON.stringify(newReq);",
							"}",
							"if (!email) {",
							"    const newReq = Object.assign({}, req);",
							"    delete newReq.email",
